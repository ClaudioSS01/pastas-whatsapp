{
  "manifest_version": 3,
  "name": "Pastas Whatsapp",
  "version": "1.0",
  "description": "Adiciona pastas no whatsapp",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://web.whatsapp.com/*"]
}
